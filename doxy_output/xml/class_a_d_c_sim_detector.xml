        <definition>ADCSimDetector::ADCSimDetector</definition>
        <argsstring>(const char *portName, int numTimePoints, NDDataType_t dataType, int maxBuffers, size_t maxMemory, int priority, int stackSize)</argsstring>
        <name>ADCSimDetector</name>
        <param>
          <type>const char *</type>
          <declname>portName</declname>
        </param>
        <param>
          <type>int</type>
          <declname>numTimePoints</declname>
        </param>
        <param>
          <type><ref refid="_n_d_attribute_8h_1a34b835bcabc3b7f546f4b25c5767cb16" kindref="member">NDDataType_t</ref></type>
          <declname>dataType</declname>
        </param>
        <param>
          <type>int</type>
          <declname>maxBuffers</declname>
        </param>
        <param>
